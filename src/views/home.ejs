<% layout('layouts/boilerplate') %>
<form action="/daily" method="post" class="row">
    <div class="container text-center">
        <div>
            <h1>Finish Mill</h1>
        </div>
        <h5>IMS Gauge</h5>
        <div class="container pt-3">
            <p>Frame Temperature</p>

            <div class="row p-3">
                <div class="form-floating pb-3 col-sm">
                    <input type="number" class="form-control" placeholder="Top Beam" name="IMS Top Beam" id="imsTopBeam">
                    <label for="imsTopBeam">Top Beam</label>
                </div>
                <div class="form-floating pb-3 col-sm">
                    <input type="number" class="form-control" placeholder="Vertical Part" name="IMS Vertical Part" id="imsVertPart">
                    <label for="imsVertPart" class="col-sm col-form-label">Vertical Part</label>
                </div>
                <div class="form-floating pb-3 col-sm">
                    <input type="number" class="form-control" placeholder="Bottom Beam" name="IMS Bottom Beam" id="imsBotBeam">
                    <label for="imsBotBeam" class="col-sm col-form-label">Bottom Beam</label>
                </div>
            </div>
        </div>

        
        <div class="container pt-3">
            <p>Water Flow Monitoring</p>
            <div class="row p-3">
                <div class="form-floating pb-3 col-lg">
                    <input type="number" class="form-control" placeholder="C-Frame" name="IMS C-Frame Flow" id="imsFlowCframe">
                    <label for="imsFlowCframe" class="col-sm col-form-label">C-Frame</label>
                </div>
                <div class="form-floating pb-3 col-lg">
                    <input type="number" class="form-control" placeholder="X-Ray Tube 1" name="IMS X-Ray Tube 1 Flow" id="imsFlowXTube1">
                    <label for="imsFlowXTube1" class="col-sm col-form-label">X-Ray Tube 1</label>
                </div>
                <div class="form-floating pb-3 col-lg">
                    <input type="number" class="form-control" placeholder="X-Ray Tube 2" name="IMS X-Ray Tube 2 Flow" id="imsFlowXTube2">
                    <label for="imsFlowXTube2" class="col-sm col-form-label">X-Ray Tube 2</label>
                </div>
                <div class="form-floating pb-3 col-lg">
                    <input type="number" class="form-control" placeholder="Heat Exchanger/Lasers" name="IMS Heat Exchanger/Lasers Flow" id="imsFlowHeatExchng">
                    <label for="imsFlowHeatExchng" class="col-sm col-form-label">Heat Exchanger/Lasers</label>
                </div>
                <div class="form-floating pb-3 col-lg">
                    <input type="number" class="form-control" placeholder="Camera Housing" name="IMS Camera Housing Flow" id="imsFlowCamera">
                    <label for="imsFlowCamera" class="col-sm col-form-label">Camera Housing</label>
                </div>
            </div>
        </div>

        
        <div class="container pt-3">
            <p>Water Temp Monitoring</p>
            <div class="row p-3">
                <div class="form-floating pb-3 col-lg">
                    <input type="number" class="form-control" placeholder="C-Frame" name="IMS C-Frame Temp" id="imsTempCframe">
                    <label for="imsTempCframe" class="col-sm col-form-label">C-Frame</label>
                </div>
                <div class="form-floating pb-3 col-lg">
                    <input type="number" class="form-control" placeholder="X-Ray Tube 1" name="IMS X-Ray Tube 1 Temp" id="imsTempXTube1">
                    <label for="imsTempXTube1" class="col-sm col-form-label">X-Ray Tube 1</label>
                </div>
                <div class="form-floating pb-3 col-lg">
                    <input type="number" class="form-control" placeholder="X-Ray Tube 2" name="IMS X-Ray Tube 2 Temp" id="imsTempXTube2">
                    <label for="imsTempXTube2" class="col-sm col-form-label">X-Ray Tube 2</label>
                </div>
                <div class="form-floating pb-3 col-lg">
                    <input type="number" class="form-control" placeholder="Heat Exchanger/Lasers" name="IMS Heat Exchanger/Lasers Temp" id="imsTempHeatExchng">
                    <label for="imsTempHeatExchng" class="col-sm col-form-label">Heat Exchanger/Lasers</label>
                </div>
                <div class="form-floating pb-3 col-lg">
                    <input type="number" class="form-control" placeholder="Camera Housing" name="IMS Camera Housing Temp" id="imsTempCamera">
                    <label for="imsTempCamera" class="col-sm col-form-label">Camera Housing</label>
                </div>
            </div>
        </div>

        <div class="container pt-3">
            <p>Customer Water Monitoring</p>

            <div class="row p-3">
                <div class="form-floating pb-3 col-sm">
                    <input type="number" class="form-control" placeholder="Camera Housing" name="IMS Customer Water Flow" id="imsCustFlow">
                    <label for="imsCustFlow" class="col-sm col-form-label">Flow</label>
                </div>
                <div class="form-floating pb-3 col-sm">
                    <input type="number" class="form-control" placeholder="Camera Housing" name="IMS Customer Water Temperature" id="imsCustTemp">
                    <label for="imsCustTemp" class="col-sm col-form-label">Temperature</label>
                </div>
            </div>
        </div>

        <div class="container pt-3">
            <p>Pressure Monitoring</p>

            <div class="row p-3">
                <div class="form-floating pb-3 col-md">
                    <input type="number" class="form-control" placeholder="Camera Housing" name="IMS C-Frame Coolant Pressure" id="imsPressCframe">
                    <label for="imsPressCframe" class="col-sm col-form-label">Coolant C-Frame</label>
                </div>
                <div class="form-floating pb-3 col-md">
                    <input type="number" class="form-control" placeholder="Camera Housing" name="IMS Periphery Coolant Pressure" id="imsPressPeriphery">
                    <label for="imsPressPeriphery" class="col-sm col-form-label">Coolant Periphery</label>
                </div>
                <div class="form-floating pb-3 col-md">
                    <input type="number" class="form-control" placeholder="Camera Housing" name="IMS Operating Air Pressure" id="imsPressOpAir">
                    <label for="imsPressOpAir" class="col-sm col-form-label">Operating Air</label>
                </div>
                <div class="form-floating pb-3 col-md">
                    <input type="number" class="form-control" placeholder="Camera Housing" name="IMS Purge Air Pressure" id="imsPressPurgeAir">
                    <label for="imsPressPurgeAir" class="col-sm col-form-label">Purge Air</label>
                </div>
            </div>
        </div>

        <h5>FM RGL</h5>
        <div class="container pt-3">
            <div class="row">
                <div class="col-md">
                    <p>Oil Flow</p>
            <div class="row">
                <div class="form-floating pb-3 col-sm">
                    <input type="number" class="form-control" placeholder="F1 Top" name="FM RGL Oil Flow F1 Top" id="F1RglOilTop">
                    <label for="imsPressCframe" class="col-sm col-form-label">F1 Top</label>
                </div>
                <div class="form-floating pb-3 col-sm">
                    <input type="number" class="form-control" placeholder="F1 Bottom" name="FM RGL Oil Flow F1 Top" id="F1RglOilBot">
                    <label for="imsPressCframe" class="col-sm col-form-label">F1 Bottom</label>
                </div>
            </div>
            <div class="row">
                <div class="form-floating pb-3 col-sm">
                    <input type="number" class="form-control" placeholder="F2 Top" name="FM RGL Oil Flow F2 Top" id="F2RglOilTop">
                    <label for="imsPressCframe" class="col-sm col-form-label">F2 Top</label>
                </div>
                <div class="form-floating pb-3 col-sm">
                    <input type="number" class="form-control" placeholder="F2 Bottom" name="FM RGL Oil Flow F2 Bottom" id="F2RglOilBot">
                    <label for="imsPressCframe" class="col-sm col-form-label">F2 Bottom</label>
                </div>
            </div>
            <div class="row">
                <div class="form-floating pb-3 col-sm">
                    <input type="number" class="form-control" placeholder="F3 Top" name="FM RGL Oil Flow F3 Top" id="F3RglOilTop">
                    <label for="imsPressCframe" class="col-sm col-form-label">F3 Top</label>
                </div>
                <div class="form-floating pb-3 col-sm">
                    <input type="number" class="form-control" placeholder="F3 Bottom" name="FM RGL Oil Flow F3 Bottom" id="F3RglOilBot">
                    <label for="imsPressCframe" class="col-sm col-form-label">F3 Bottom</label>
                </div>
            </div>
                </div>
                <div class="col-md">
                    <p>Water Flow</p>
            <div class="row">
                <div class="form-floating pb-3 col-sm">
                    <input type="number" class="form-control" placeholder="F1 Top" name="FM RGL Water Flow F1 Top" id="F1RglWaterTop">
                    <label for="imsPressCframe" class="col-sm col-form-label">F1 Top</label>
                </div>
                <div class="form-floating pb-3 col-sm">
                    <input type="number" class="form-control" placeholder="F1 Bottom" name="FM RGL Water Flow F1 Top" id="F1RglWaterBot">
                    <label for="imsPressCframe" class="col-sm col-form-label">F1 Bottom</label>
                </div>
            </div>
            <div class="row">
                <div class="form-floating pb-3 col-sm">
                    <input type="number" class="form-control" placeholder="F2 Top" name="FM RGL Water Flow F2 Top" id="F2RglWaterTop">
                    <label for="imsPressCframe" class="col-sm col-form-label">F2 Top</label>
                </div>
                <div class="form-floating pb-3 col-sm">
                    <input type="number" class="form-control" placeholder="F2 Bottom" name="FM RGL Water Flow F2 Bottom" id="F2RglWaterBot">
                    <label for="imsPressCframe" class="col-sm col-form-label">F2 Bottom</label>
                </div>
            </div>
            <div class="row">
                <div class="form-floating pb-3 col-sm">
                    <input type="number" class="form-control" placeholder="F3 Top" name="FM RGL Water Flow F3 Top" id="F3RglWaterTop">
                    <label for="imsPressCframe" class="col-sm col-form-label">F3 Top</label>
                </div>
                <div class="form-floating pb-3 col-sm">
                    <input type="number" class="form-control" placeholder="F3 Bottom" name="FM RGL Water Flow F3 Bottom" id="F3RglWaterBot">
                    <label for="imsPressCframe" class="col-sm col-form-label">F3 Bottom</label>
                </div>
            </div>
                </div>
            </div>
            
        </div>

        <div class="form-check form-check-inline">
  <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio1" value="option1">
  <label class="form-check-label" for="inlineRadio1">1</label>
</div>
<div class="form-check form-check-inline">
  <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio2" value="option2">
  <label class="form-check-label" for="inlineRadio2">2</label>
</div>
<div class="form-check form-check-inline">
  <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio3" value="option3" disabled>
  <label class="form-check-label" for="inlineRadio3">3 (disabled)</label>
</div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </div>
    </form>    